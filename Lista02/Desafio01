public class Main {
    public static void main(String [] args){
        int numero1 = 0, numero2 = 1;
        int resultado = numero1 + numero2;

        System.out.print(numero1 + " " + numero2);

        while (resultado <= 500) {
            System.out.print(" " + resultado);
            numero1 = numero2;
            numero2 = resultado;
            resultado = numero1 + numero2;
        }
        System.out.println(" " + resultado);
    }
}

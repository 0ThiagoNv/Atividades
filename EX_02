import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        System.out.println("Digite dois números e veja qual é o maior");

        System.out.println("Digite o primeiro número: ");
        Scanner digiteNumero1 = new Scanner(System.in);
        int numero1 = digiteNumero1.nextInt();

        System.out.println("Digite o segundo número: ");
        Scanner digiteNumero2 = new Scanner(System.in);
        int numero2 = digiteNumero2.nextInt();

        if(numero1 > numero2) {
            System.out.println(numero1 + " é maior!");
        }else{
            System.out.println(numero2 + " é maior!");
        }
    }
}
